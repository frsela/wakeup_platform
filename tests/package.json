  "scripts": {
    "test": "npm run-script test_local ; npm run-script test_global",
    "test_local": "./node_modules/.bin/vows local/unit/test_*.js -i --spec ; ./node_modules/.bin/vows local/functional/test_*.js -i --spec",
    "test_global": "./node_modules/.bin/vows global/unit/test_*.js -i --spec ; ./node_modules/.bin/vows global/functional/test_*.js -i --spec",
    "start": "npm run-script start_local ; npm run-script start_global",
